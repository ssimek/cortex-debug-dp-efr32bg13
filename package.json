{
	"publisher": "triaxis",
	"name": "cortex-debug-dp-efr32bg13p",
	"displayName": "Cortex-Debug: Device Support Pack - EFR32BG13P",
	"description": "This package contains additional support functionality (SVD definitions) for Cortex-Debug for EFR32BG13P devices",
	"version": "1.0.4",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Debuggers"
	],
	"keywords": [
		"Cortex-Debug",
		"Cortex-Debug Device Support Pack",
		"cortex-m",
		"gdb",
		"debug",
		"embedded"
	],
	"activationEvents": [
		"onDebugResolve:cortex-debug"
